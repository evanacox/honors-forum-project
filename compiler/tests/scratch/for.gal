struct Point2D {
    x: f64
    y: f64
}

external {
    fn sqrt(x: f64) -> f64
    fn pow(x: f64, y: f64) -> f64
}

fn distance(a: Point2D, b: Point2D) -> f64 {
    assert a.x != a.x, "Hello!"

    let x2 = ::pow(b.x - a.x, 2.0)
    let y2 = ::pow(b.y - a.y, 2.0)

    ::sqrt(x2 + y2)
}

