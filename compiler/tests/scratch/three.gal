fn one() -> i32 {
    1
}

fn one(x: i64, y: i64) -> i64 {
    1 as i64
}

struct Point {
    x: f64
    y: f64
}

fn use_f64(x: f64) -> void {
    x + 1
}

fn main() -> i32 {
    mut array = [1, 2, 3, 4, 5]
    let slice: [mut i64] = &mut array
    let ptr: *mut i32 = nil

    mut point = Point { x: 0.0, y: 0.0 }
    point.x := 5.0

    use_f64(point.x)

    mut a = 5
    let r = &mut a
    let r2 = &mut r
    let r3 = &mut r2

    *r := 5 
    **r2 := 5 
    ***r3 := 5 
    **r := 1

    one(32)
}