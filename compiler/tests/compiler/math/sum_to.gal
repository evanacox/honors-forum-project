// test: should-run 
// returns: 0
// outputs: none 

fn sum_upto(x: i64) -> i64 {
    mut accum = 0

    for i := 1 to x + 1 {
        accum += i
    }

    accum
}

fn main() -> i32 {
    assert sum_upto(15) == 120
    assert sum_upto(5) == 15
    assert sum_upto(0) == 0
    assert sum_upto(10) != 19
    assert sum_upto(12) != 0

    0
}