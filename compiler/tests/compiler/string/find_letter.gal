fn find_letter(c: char, str: [char]) -> isize {
    for i := 0 to str.len {
        if str[i] == c {
            return i 
        }
    }

    -1 as isize
}

fn main() -> i32 {
    assert find_letter('e', "Hello") == 1
    assert find_letter('!', "") == -1 as isize

    0
}