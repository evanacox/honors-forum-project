// test: should-run
// returns: 0
// outputs: [4718088, 106, 66, 160, 8442, 3186]

fn transform(arr: [mut i64]) -> void {
    for i := 0 to arr.size {
        arr[i] *= 2
    }

    for i := 0 to arr.size {
        arr[i] += 42
    }

    for i := 0 to arr.size {
        arr[i] &= 0xFFFFFFFF
    }
}

fn print(arr: [i64]) -> void {
    print('[')

    for i := 0 to arr.size - 1 {
        print(arr[i])
        print(", ")
    } 

    print(arr[arr.size - 1])
    print("]")
}

fn main() -> i32 {
    mut arr = [2359023, 32, 12, 59, 4200, 1572]
    transform(&mut arr)
    print(&arr)

    0
}