// outputs: none

fn array_eq(a: [i64], b: [i64]) -> bool {
    if a.size != b.size {
        return false
    }

    for i := 0 to a.size {
        if a[i] != b[i] {
            return false 
        }
    }

    true
}

fn main() -> i32 {
    let arr = [1, 2, 3, 4, 5]
    let arr2 = [3, 4, 5]
    let a = arr[0..5]
    let b = arr[2..5]
    let c = arr[0..=4]

    assert a.size == 5
    assert array_eq(&arr, a)
    assert b.size == 3
    assert array_eq(&arr2, b)
    assert c.size == 5
    assert array_eq(a, c)

    0
}